  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "start": "npm run develop",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "deploy": "node scripts/fetchLanguageColors && gatsby build --prefix-paths && gh-pages -d public -r https://$GH_TOKEN@github.com/bdTechies/prozuktischool.com.git",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public && rm -rf .cache",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "create": "node scripts/newpost.js",
    "test": "jest --update-snapshot",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "analyze": "ANALYZE_BUNDLE_SIZE=true gatsby build"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
